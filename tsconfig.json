{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "pretty": true,
    "watch": true,
    "allowJs": true,
    "jsx": "react"
  },
  "exclude": [
    //    "public",
    "node_modules",
    "front",
    "runtime"
  ]
}